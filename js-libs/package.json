    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "7.11.1",
    "jest-canvas-mock": "^1.1.0",
    "jsdoc": "^3.5.5",
    "react-scripts": "^3.0.1",
    "typescript": "^3.4.5",
    "webpack-cli": "^3.3.2"
  },
  "resolutions": {
    "fsevents": "1.2.9",
    "**/fsevents": "1.2.9"
  }
